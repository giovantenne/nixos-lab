{
  "update.mode": "none",
  "update.enableWindowsBackgroundUpdates": false,
  "update.showReleaseNotes": false,
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "workbench.startupEditor": "none",
  "workbench.welcomePage.enabled": false,
  "workbench.welcomePage.walkthroughs.openOnStartup": false,
  "workbench.welcomePage.walkthroughs.enabled": false,
  "workbench.tips.enabled": false,
  "workbench.activityBar.visible": true,
  "workbench.sideBar.location": "left",
  "workbench.auxiliaryBar.visible": false,
  "workbench.secondarySideBar.visible": false,
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.panel.defaultLocation": "bottom",
  "chat.commandCenter.enabled": false,
  "chat.agent.enabled": false,
  "chat.editor.fontFamily": "",
  "github.copilot.enable": {
    "*": false
  },
  "github.copilot.chat.enabled": false,
  "github.copilot.chat.agent.enabled": false,
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "/run/current-system/sw/bin/bash",
      "args": ["-l"]
    }
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "extensions.ignoreRecommendations": true,
  "telemetry.telemetryLevel": "off",
  "redhat.telemetry.enabled": false
